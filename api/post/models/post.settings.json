{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "author": {
      "unique": true,
      "plugin": "users-permissions",
      "model": "user"
    },
    "description": {
      "type": "string"
    },
    "hashtag": {
      "type": "string"
    },
    "thumbnail": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "post_like": {
      "via": "post",
      "model": "post-like"
    }
  }
}
